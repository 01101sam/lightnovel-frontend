    "luxon": "^2.0.2",
    "masonry-layout": "^4.2.2",
    "naive-ui": "^2.19.3",
    "nanoid": "^3.1.28",
    "pinia": "2.0.0-rc.11",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.19",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/luxon": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@volar/experimental": "^0.27.25",
    "@vue/cli-plugin-babel": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
